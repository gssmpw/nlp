@inproceedings{meyer_mondrian_2006,
	address = {Brighton, United Kingdom},
	title = {Mondrian: an agile information visualization framework},
	isbn = {978-1-59593-464-2},
	shorttitle = {Mondrian},
	url = {http://portal.acm.org/citation.cfm?doid=1148493.1148513},
	doi = {10.1145/1148493.1148513},
	abstract = {Data visualization is the process of representing data as pictures to support reasoning about the underlying data. For the interpretation to be as easy as possible, we need to be as close as possible to the original data. As most visualization tools have an internal metamodel, which is different from the one for the presented data, they usually need to duplicate the original data to conform to their metamodel. This leads to an increase in the resources needed, increase which is not always justiﬁed. In this work we argue for the need of having an engine that is as close as possible to the data and we present our solution of moving the visualization tool to the data, instead of moving the data to the visualization tool. Our solution also emphasizes the necessity of reusing basic blocks to express complex visualizations and allowing the programmer to script the visualization using his preferred tools, rather than a third party format. As a validation of the expressiveness of our framework, we show how we express several already published visualizations and describe the pros and cons of the approach.},
	language = {en},
	urldate = {2024-10-05},
