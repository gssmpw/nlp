	author = {Bellows, Brooke G. and Higgins, Jordan F. and Youmans, Robert J.},
	editor = {Hutchison, David and Kanade, Takeo and Kittler, Josef and Kleinberg, Jon M. and Mattern, Friedemann and Mitchell, John C. and Naor, Moni and Nierstrasz, Oscar and Pandu Rangan, C. and Steffen, Bernhard and Sudan, Madhu and Terzopoulos, Demetri and Tygar, Doug and Vardi, Moshe Y. and Weikum, Gerhard and Marcus, Aaron},
	year = {2013},
	doi = {10.1007/978-3-642-39229-0_2},
	note = {Series Title: Lecture Notes in Computer Science},
	pages = {13--21},
}

