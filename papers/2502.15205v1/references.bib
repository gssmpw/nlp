@techreport{regal_situational_1988,
	title = {{SITUATIONAL} {AWARENESS} {IN} {THE} {COMMERCIAL} {FLIGHT} {DECK}: {DEFINITION}, {MEASUREMENT}, {AND} {ENHANCEMENT}},
