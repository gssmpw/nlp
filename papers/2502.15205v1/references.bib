@article{lavalle_methodology_2022,
	title = {A {Methodology}, {Based} on {User} {Requirements} and {Visualization} {Techniques}, to {Evaluate} the {Impact} of {Bias} in {Data} {Analytics} and {Artificial} {Intelligence} {Algorithms}},
	issn = {1556-5068},
	url = {https://www.ssrn.com/abstract=4236027},
	doi = {10.2139/ssrn.4236027},
	abstract = {Data Analytics and Artificial Intelligence (AI) have become a key component of many business processes and they influence many aspects of our daily life. Data Analytics and AI outputs make or break the outcome of the process. In turn, any misinterpretation or flaw in these outputs can cause significant damage. One of the most often overlooked flaws is the unaware use of biased data. When data bias goes unadverted, it warps the meaning of data and it has a devastating effect on AI results. It is widely argued that the most common manner to deal with data bias is to rebalance biased datasets. Unfortunately, rebalancing is not an aseptic transformation, so it leads to several potentially undesired side-effects such as altering the distribution of the data or lowering the overall accuracy.},
