	author = {Wall, Emily and Stasko, John and Endert, Alex},
	month = oct,
	year = {2019},
	keywords = {Data analysis, Data visualization, Decision making, Human Computer Interaction (HCI), Human-centered computing, Index Terms: Human-centered computing, Task analysis, Tools, Training, Visualization},
