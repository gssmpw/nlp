@article{silk_problem_2021,
	title = {Problem framing and cognitive style: {Impacts} on design ideation perceptions},
	volume = {74},
	issn = {0142694X},
	shorttitle = {Problem framing and cognitive style},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S0142694X21000260},
	doi = {10.1016/j.destud.2021.101015},
	abstract = {Designers’ engagement in ideation is impacted by their perceptions of the parameters of the design task. Whereas expert designers actively interrogate and reframe given problems, novice designers tend to solve them as given. We investigated novice designers’ perceptions of their ideas relative to problem frames and their cognitive styles. We found that innovative styles and framings corresponded to higher perceptions of idea diversity and creativity, whereas adaptive framings corresponded to lower perceptions of idea creativity and perceptions of greater diﬃculty generating ideas. Novice designers may hold implicit theories of creativity that undervalue adaptive creativity. However, problem frames that misalign with a novice’s preferred cognitive style can impact their ideation, and so hold promise for challenging their initial interpretations and approaches. Ó 2021 Elsevier Ltd. All rights reserved.},
