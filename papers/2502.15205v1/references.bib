@article{glaser_racial_2014,
	title = {Racial {Bias} and {Public} {Policy}},
	volume = {1},
	issn = {2372-7322},
	url = {https://doi.org/10.1177/2372732214550403},
	doi = {10.1177/2372732214550403},
	abstract = {This article explores psychological science on race bias and its implications in several domains of public policy, with special attention paid to biased policing as an illustrative example. Race bias arises from normal mental processes, many outside our conscious awareness and control. This research directly applies to public policy, especially where concerned with regulating behavior and managing uncertainty. Research links both implicit and explicit racial bias to behavior, and uncertainty exacerbates the influence of bias in decision-making. Sample policy domains—where psychological research, race bias, and public policy intersect—include education, employment, immigration, health care, politics/representation, and criminal justice. Psychological research informs policy by documenting causes and processes, by expert testimony in court, and by generating and evaluating interventions to reduce race bias.},
