@inproceedings{bettaieb_sources_2022,
	title = {{SOURCES} {OF} {INSPIRATION} {IN} {THE} {INTERIOR} {DESIGN} {PROCESS}: {HOW} {IDEATION} {IS} {AFFECTED} {BY} {MODALITY}},
	isbn = {978-605-06-2867-8},
	shorttitle = {{SOURCES} {OF} {INSPIRATION} {IN} {THE} {INTERIOR} {DESIGN} {PROCESS}},
	url = {https://www.ocerints.org/darch22_e-publication/abstracts/aDonia M Bettaieb.html},
	doi = {10.46529/darch.202206},
	abstract = {Different sources of inspiration influence the generation of ideas during the design process differently. In this study, we compare the influence of the two modes of representation of external stimuli, picture vs. text, on the conceptual design process in the idea generation phase. We analyse how these two types of inspiration sources affect the development and outcomes of the ideation process. We collected data during a workshop with twenty-four senior-level interior design students at King Abdul-Aziz University. We gave the participants an exploratory design problem to solve involving the design of a college residency room for two people. Some participants received stimuli in the form of text and others in the form of images. The participants’ outcomes for the task were assessed under the conditions of the two modes of representation by external judges. The study also analysed participants’ responses to a questionnaire about their experience during the experiment, their relevant opinions, and problems they encountered, if any. The results indicate that compared to the use of text, the use of pictures to activate the inspiration process helped participants produce complex thinking, original solutions, and the use of drawing for idea generation.},
	language = {en},
	urldate = {2023-10-09},
	booktitle = {Proceedings of {DARCH} 2022- 2nd {International} {Conference} on {Architecture} \& {Design}},
	publisher = {International Organization Center of Academic Research},
	author = {Bettaieb, Donia M},
	month = mar,
	year = {2022},
}

@article{kliman-silver_automation_nodate,
	title = {Automation and inspiration: {Understanding} the value of artificial intelligence in user experience design tools},
	abstract = {Recently, artificial intelligence (AI) has been introduced into a variety of software platforms aimed at supporting creative endeavors. While AI-driven features in design tooling are nascent, we view AI as having the potential for positive impact on user experience and product designer workflows. We conducted a series of studies to investigate how designers perceive AI-driven experiences in design tooling. The findings suggest that designers view AI as suited to identifying and correcting errors and as agents for inspiring creativity. However, trust and explainability are key to acceptance. Our goal is to inform the future of design tooling through further development of these initial inclinations.},
	language = {en},
