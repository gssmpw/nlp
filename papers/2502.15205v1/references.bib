	shorttitle = {Design in the age of {Artificial} {Intelligence}},
	url = {https://rgdoi.net/10.13140/RG.2.2.33028.91523},
	doi = {10.13140/RG.2.2.33028.91523},
	abstract = {User experience (UX) is vital for improving the functionality and appeal of digital and physical products, boosting productivity. In today's technology industry, UX is indispensable, driving increased user engagement. The integration of artificial intelligence (AI) has garnered attention, leading to advancements that enhance UX. AI, particularly machine-learning-enabled tools, is making notable progress in supporting UX designers and improving overall user satisfaction. The purpose of this study is to conduct an illustrated literature review of eleven studies on AI-supported UX processes in order to highlight research problems, benefits, methodologies, gaps, and solutions to address them. These may provide insight and information that may help guide future studies. This study is prompted by the fact that there are numerous UX process issues that can obstruct designers' work. However, the study could find effective responses throughout the six queries through discussion.},
	language = {en},
	urldate = {2024-01-28},
	author = {{Pehtlada Chanchamnan} and {Soeng San} and Ho, Conway},
	year = {2023},
	note = {Publisher: Unpublished},
}

