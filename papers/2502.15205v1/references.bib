@article{Lowgren2013,
	title = {Annotated {Portfolios} and {Other} {Forms} of {Intermediate}-{Level} {Knowledge}},
	issn = {10725520},
	doi = {10.1145/2405716.2405725},
	abstract = {In a recent issue of this magazine, Bill Gaver and John Bowers address the role of design practice in academic research and provide a concrete suggestion: “We propose the notion of annotated portfolios as a way to communicate design research. In part, we do this to provide an alternative to accounts that suggest for design to become productive as research, it should engage in some sort of theory formation. While what exactly is meant by theory is not always clear, writers usually have in mind some conceptual machinery that can explain and predict”. Briefly, the notion of annotated portfolios entails selecting a collection of designs, rpresenting them in an appropriate medium, and combining the design re-presentations with brief textual annotations. Gaver and Bowers characterize their proposal as a methodology for communicating design research, and more specifically, a methodology that is very familiar to designers as well as artists.},
