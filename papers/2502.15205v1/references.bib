}

@incollection{ellis_bias_2018,
	address = {Cham},
	title = {Bias by {Default}?: {A} {Means} for {A} {Priori} {Interface} {Measurement}},
	isbn = {978-3-319-95830-9 978-3-319-95831-6},
	shorttitle = {Bias by {Default}?},
	url = {http://link.springer.com/10.1007/978-3-319-95831-6_4},
	abstract = {Systems have biases. Their interfaces naturally guide a user toward speciﬁc patterns of action. For example, modern word-processors and spreadsheets are both capable of handling word wrapping, checking spelling, and calculating formulas. You could write a paper in a spreadsheet or could do simple business modeling in a wordprocessor. However, their interfaces naturally communicate which function they are designed for. Visual analytic interfaces also have biases. We outline why simple Markov models are a plausible tool for investigating that bias, even prior to user interactions, and how they might be applied to understand a priori system biases. We also discuss some anticipated difﬁculties in such modeling and touch brieﬂy on what some Markov model extensions might provide.},
	language = {en},
	urldate = {2023-03-16},
	booktitle = {Cognitive {Biases} in {Visualizations}},
