@article{daly_comparing_2016,
	title = {Comparing {Ideation} {Techniques} for {Beginning} {Designers}},
	volume = {138},
	issn = {1050-0472, 1528-9001},
	url = {https://asmedigitalcollection.asme.org/mechanicaldesign/article/doi/10.1115/1.4034087/376264/Comparing-Ideation-Techniques-for-Beginning},
	doi = {10.1115/1.4034087},
	abstract = {Concept generation techniques can help to support designers in generating multiple ideas during design tasks. However, differences in the ways these techniques guide idea generation are not well understood. This study investigated the qualities of concepts generated by beginning engineering designers using one of three different idea generation techniques. Working individually on an open-ended engineering design problem, 102 first year engineering students learned and applied one of three different ideation techniques—design heuristics, morphological analysis, or individual brainstorming (using brainstorming rules to generate ideas working alone)—to a given design problem. Using the consensual assessment technique, all concepts were rated for creativity, elaboration, and practicality, and all participants' concept sets were rated for quantity and diversity. The simplest technique, individual brainstorming, led to the most concepts within the short (25 minute) ideation session. All three techniques produced creative concepts averaging near the scale midpoint. The elaboration of the concepts was significantly higher with design heuristics and morphological analysis techniques, and the practicality was significantly higher using design heuristics. Controlling for number of concepts generated, there were no significant differences in diversity of solution sets across groups. These results demonstrate that the use of design heuristics does not limit the creativity of ideation outcomes, and helps students to develop more elaborate and practical ideas. Design heuristics show advantages in the initial idea generation phase for beginning engineering students. These findings point to specific strengths in different ideation techniques, and the value of exposing beginning designers to multiple techniques for idea generation.},
	language = {en},
	number = {10},
	urldate = {2022-11-06},
	journal = {Journal of Mechanical Design},
	author = {Daly, Shanna R. and Seifert, Colleen M. and Yilmaz, Seda and Gonzalez, Richard},
	month = oct,
	year = {2016},
	pages = {101108},
}

@article{mccaffrey_innovation_2012,
	title = {Innovation {Relies} on the {Obscure}: {A} {Key} to {Overcoming} the {Classic} {Problem} of {Functional} {Fixedness}},
	volume = {23},
	issn = {0956-7976, 1467-9280},
	shorttitle = {Innovation {Relies} on the {Obscure}},
	url = {http://journals.sagepub.com/doi/10.1177/0956797611429580},
	doi = {10.1177/0956797611429580},
	abstract = {A recent analysis of real-world problems that led to historic inventions and insight problems that are used in psychology experiments suggests that during innovative problem solving, individuals discover at least one infrequently noticed or new (i.e., obscure) feature of the problem that can be used to reach a solution. This observation suggests that research uncovering aspects of the human semantic, perceptual, and motor systems that inhibit the noticing of obscure features would enable researchers to identify effective techniques to overcome those obstacles. As a critical step in this research program, this study showed that the generic-parts technique can help people unearth the types of obscure features that can be used to overcome functional fixedness, which is a classic inhibitor to problem solving. Subjects trained on this technique solved on average 67\% more problems than a control group did. By devising techniques that facilitate the noticing of obscure features in order to overcome impediments to problem solving (e.g., design fixation), researchers can systematically create a tool kit of innovationenhancing techniques.},
	language = {en},
	number = {3},
	urldate = {2022-11-06},
	journal = {Psychological Science},
	author = {McCaffrey, Tony},
	month = mar,
	year = {2012},
	pages = {215--218},
}

@inproceedings{siangliulue_toward_2015,
	address = {Vancouver BC Canada},
	title = {Toward {Collaborative} {Ideation} at {Scale}: {Leveraging} {Ideas} from {Others} to {Generate} {More} {Creative} and {Diverse} {Ideas}},
	isbn = {978-1-4503-2922-4},
	shorttitle = {Toward {Collaborative} {Ideation} at {Scale}},
	url = {https://dl.acm.org/doi/10.1145/2675133.2675239},
	doi = {10.1145/2675133.2675239},
	abstract = {A growing number of large collaborative idea generation platforms promise that by generating ideas together, people can create better ideas than any would have alone. But how might these platforms best leverage the number and diversity of contributors to help each contributor generate even better ideas? Prior research suggests that seeing particularly creative or diverse ideas from others can inspire you, but few scalable mechanisms exist to assess diversity. We contribute a new scalable crowd-powered method for evaluating the diversity of sets of ideas. The method relies on similarity comparisons (is idea A more similar to B or C?) generated by non-experts to create an abstract spatial idea map. Our validation study reveals that human raters agree with the estimates of dissimilarity derived from our idea map as much or more than they agree with each other. People seeing the diverse sets of examples from our idea map generate more diverse ideas than those seeing randomly selected examples. Our results also corroborate ﬁndings from prior research showing that people presented with creative examples generated more creative ideas than those who saw a set of random examples. We see this work as a step toward building more effective online systems for supporting large scale collective ideation.},
	language = {en},
	urldate = {2022-11-06},
	booktitle = {Proceedings of the 18th {ACM} {Conference} on {Computer} {Supported} {Cooperative} {Work} \& {Social} {Computing}},
	publisher = {ACM},
	author = {Siangliulue, Pao and Arnold, Kenneth C. and Gajos, Krzysztof Z. and Dow, Steven P.},
	month = feb,
	year = {2015},
	pages = {937--945},
}

@article{neroni_playing_nodate,
	title = {Playing games with fixation: {Linking} psychology and design research to develop computer‐based tasks to study how designers are affected by mental set},
	abstract = {The term ‘fixation’ was originally used by psychologists to describe the variety of blocks that can impede insight, often resulting from the counterproductive effects of prior knowledge (Smith, 1995). In psychology research, fixation is studied through the use of creative problem-solving tasks in which performance is typically measured by assessing the participants’ ability to identify unusual or novel solutions or by assessing the time taken to accomplish the task. In design research, the term ‘design fixation’ is understood as a broader set of unfavorable phenomena; it describes how designers may unconsciously adhere to a limited set of ideas during concept generation (Jansson and Smith, 1991). In an attempt to study design fixation in a way that represents something of the nature of design practice, researchers typically set participants open-ended design problems and measure their performance through a variety of highly subjective metrics. This particular experimental approach to studying design fixation gives rise to various methodological problems, including limited opportunities for data capture and highly subjective assessment procedures. There is also variation in how these procedures are implemented, leading to difficulty in comparing the results of different studies. In order to overcome these methodological limitations, this position paper proposes studying design fixation with computer games inspired by experimental paradigms used in psychology research. This will permit better data capture, a more objective analysis of design behavior and will also allow for a better connection to be established between design fixation research and the broader set of psychological studies.},
	language = {en},
	author = {Neroni, Maria A and Vasconcelos, Luis A and Crilly, Nathan},
	pages = {9},
}

