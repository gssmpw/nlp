@article{prendeville_politics_2022,
	title = {On the {Politics} of {Design} {Framing} {Practices}},
	volume = {38},
	issn = {0747-9360},
	doi = {10.1162/desi_a_00692},
	abstract = {In this conceptual article, our aim is to deconstruct the conceptualization of design framing and establish its essentially political nature. It demonstrates the positionality inherent within frames insofar as frames articulate subordinated or dominant status, or express normative understandings until challenged. In doing so, we build a conceptualization of the political foundations of design framing practices and their implications for those contexts within which design operates. Consequently, we argue for dissensual counter-framing design practices that unsettle institutionalized norms and ideologies played out within frames, and through which a form of political agency is sociomaterially enacted.},
