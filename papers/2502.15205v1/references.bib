}

@inproceedings{mendez_bottom-up_2017,
	address = {Denver Colorado USA},
	title = {Bottom-up vs. {Top}-down: {Trade}-offs in {Efficiency}, {Understanding}, {Freedom} and {Creativity} with {InfoVis} {Tools}},
