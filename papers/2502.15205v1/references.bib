@article{huilgol_would_nodate,
	title = {Would {You} {Rather}: {How} {Data} {Visualizations} {Affect} {Choices} in {Medical} {Decision}-{Making}},
	abstract = {The present research examined how various forms of data visualizations affected medical decision-making. In a series of four experiments, participants were given different data visualizations and asked to choose between two treatment options - one with a higher survival rate and complications, or one with a lower survival rate and no complications. Broadly, results indicated most people preferred the latter and including visualizations led to some differences in medical decision-making. This finding was most evident when comparing decisions made with visualizations and decisions made without visualizations. The research also investigated if framing affected medical decision-making and found that across all framing conditions, decisions stayed consistent. However, when health complications were presented in a more or less approach rather than an all or none approach, regardless of visualization type, the majority of people opted for the higher survival rate with more complications. These findings contribute to the ongoing dialogue from previous studies examining the relationship between visualizations, risk communication, and medical decision-making.},
	language = {en},
	author = {Huilgol, Shreya},
}

