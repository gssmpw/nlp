@inproceedings{meyer_four-level_2012,
	address = {New York, NY, USA},
	series = {{BELIV} '12},
	title = {The four-level nested model revisited: blocks and guidelines},
	isbn = {978-1-4503-1791-7},
	shorttitle = {The four-level nested model revisited},
