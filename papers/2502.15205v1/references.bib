@article{chow_why_1999,
	title = {Why {Mission} {Control} {Works}: {Supporting} {Cognition} and {Collaboration} in {Distributed}, {Dynamic} {Worlds}},
	volume = {43},
	issn = {2169-5067, 1071-1813},
	shorttitle = {Why {Mission} {Control} {Works}},
	url = {http://journals.sagepub.com/doi/10.1177/154193129904300330},
	doi = {10.1177/154193129904300330},
	abstract = {Studies of failures have dominated human factors research in complex, dynamic domains. The goal of this panel, however, is to use a case study of success - space shuttle mission control - to explore how robust problem-solving and effective collaboration among distributed, functionally distinct agents can be facilitated. This panel brings together human factors researchers and systems designers who are familiar with mission control to discuss the cognitive factors, organizational practices, and technological innovations that contribute to the success of space shuttle missions. As with other complex domains, economic and technological pressures are threatening mission control's bases for success. These pressures (e.g., budget/staff reductions, more complex technology, and longer, more remote missions) are intensifying demands for “smarter”, more autonomous computer support tools. Therefore, this panel explores approaches for designing intelligent systems and automation that perform as “team players” in challenging operational environments - where effective collaboration constitutes the key to success.},
	language = {en},
	number = {3},
	urldate = {2024-02-29},
	journal = {Proceedings of the Human Factors and Ergonomics Society Annual Meeting},
	author = {Chow, Renée},
	month = sep,
	year = {1999},
	pages = {271--274},
}

@inproceedings{colombano_system_2013,
	address = {San Diego, CA},
	title = {A system for fault management and fault consequences analysis for {NASA}’s {Deep} {Space} {Habitat}},
	isbn = {978-1-62410-239-4},
	url = {https://arc.aiaa.org/doi/10.2514/6.2013-5319},
	doi = {10.2514/6.2013-5319},
	abstract = {NASA’s exploration program envisions the utilization of a Deep Space Habitat (DSH) for human exploration of the space environment in the vicinity of Mars and/or asteroids. Communication latencies with ground control of as long as 20+ minutes make it imperative that DSH operations be highly autonomous, as any telemetry-based detection of a systems problem on Earth could well occur too late to assist the crew with the problem. A DSHbased development program has been initiated to develop and test the automation technologies necessary to support highly autonomous DSH operations. One such technology is a fault management tool to support performance monitoring of vehicle systems operations and to assist with real-time decision making in connection with operational anomalies and failures. Toward that end, we are developing Advanced Caution and Warning System (ACAWS), a tool that combines dynamic and interactive graphical representations of spacecraft systems, systems modeling, automated diagnostic analysis and root cause identification, system and mission impact assessment, and mitigation procedure identification to help spacecraft operators (both flight controllers and crew) understand and respond to anomalies more effectively. In this paper, we describe four major architecture elements of ACAWS: Anomaly Detection, Fault Isolation, System Effects Analysis, and Graphic User Interface (GUI), and how these elements work in concert with each other and with other tools to provide fault management support to both the controllers and crew. We then describe recent evaluations and tests of ACAWS on the DSH testbed. The results of these tests support the feasibility and strength of our approach to failure management automation and enhanced operational autonomy.},
	language = {en},
	urldate = {2024-02-29},
	booktitle = {{AIAA} {SPACE} 2013 {Conference} and {Exposition}},
	publisher = {American Institute of Aeronautics and Astronautics},
	author = {Colombano, Silvano P. and Spirkovska, Liljana and Baskaran, Vijayakumar and Aaseng, Gordon and McCann, Robert and Ossenfort, John and Smith, Irene and Iverson, David and Schwabacher, Mark},
	month = sep,
	year = {2013},
}

@inproceedings{caldwell_simulation_2012,
	address = {Salt Lake City, UT, USA},
	title = {Simulation and human factors in modeling of spaceflight mission control teams},
	isbn = {978-1-4673-0163-3 978-1-4673-0161-9 978-1-4673-0162-6},
	url = {http://ieeexplore.ieee.org/document/6309288/},
	doi = {10.1109/ISRCS.2012.6309288},
	abstract = {This paper describes a recently completed project to develop a human factors informed simulation of team-based expert coordination and knowledge sharing tasks in a complex and resilient control system. The project explores processes of anomaly response in NASA spaceflight mission control teams, using as a baseline example the mission profile and anomalies experienced during the final Space Shuttle flight, STS-135. While controllers in this simulation work to detect and resolve anomalies using technical decision criteria, their performance is subject to stochastic and non-rational dynamics of information availability and flow affecting situation awareness and hypothesis generation. The initial goal of this work is to assist in analysis of alternatives for future mission control room designs, and to develop increased simulation capability in the area of distributed expertise and problem solving in teams.},
	language = {en},
	urldate = {2024-02-29},
	booktitle = {2012 5th {International} {Symposium} on {Resilient} {Control} {Systems}},
	publisher = {IEEE},
	author = {Caldwell, Barrett S. and Onken, Jeffrey D.},
	month = aug,
