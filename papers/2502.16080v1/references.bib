@incollection{Hahn1989,
title = {21 - ON SOME PROBLEMS OF PROVING THE EXISTENCE OF AN EQUILIBRIUM IN A MONETARY ECONOMY},
editor = {Ross M. Starr},
booktitle = {General Equilibrium Models of Monetary Economies},
publisher = {Academic Press},
pages = {297-306},
year = {1989},
isbn = {978-0-12-663970-4},
doi = {https://doi.org/10.1016/B978-0-12-663970-4.50024-4},
url = {https://www.sciencedirect.com/science/article/pii/B9780126639704500244},
author = {F.H. Hahn},
abstract = {Publisher Summary
This chapter discusses some problems of proving the existence of equilibrium in a monetary economy. Most theories in economics involve the equilibrium solutions of certain models. These solutions are used as benchmarks, or as states to which an economic system is believed to tend or as the means for making long-term predictions. Implicitly or explicitly most economists use the notion of an equilibrium. It is evident that a proof of the existence of equilibrium that turns crucially on a supposition is hardly acceptable. The role of this assumption is simply to enable one to employ a technical trick to ensure that one can use a fixed point theorem and one cannot believe that it has any fundamental significance to the whole problem. The trick of course was to keep the exchange value of money strictly positive throughout so that money was always desirable and to obtain the desired continuity be varying initial total stocks knowing that once an equilibrium was found the actual initial stock distribution could be restored without upsetting the equilibrium.}
}

