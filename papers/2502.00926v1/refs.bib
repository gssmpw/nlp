@article{LiEtAlMagnetoactiveSoftGripper2020,
  title = {A Magneto-Active Soft Gripper with Adaptive and Controllable Motion},
  author = {Li, Xianghao and Zhang, Zheng and Sun, Min and Wu, Helong and Zhou, Yisong and Wu, Huaping and Jiang, Shaofei},
  year = {2020},
