@article{FischerEtAlFunnelShapedMicrostructuresStrong2017,
  title = {Funnel-{{Shaped Microstructures}} for {{Strong Reversible Adhesion}}},
  author = {Fischer, Sarah C. L. and Gro{\ss}, Katja and Torrents Abad, Oscar and Becker, Michael M. and Park, Euiyoung and Hensel, Ren{\'e} and Arzt, Eduard},
  year = {2017},
  journal = {Advanced Materials Interfaces},
