}

@article{ZhuEtAlEffectSurfaceTension2022,
  title = {The Effect of Surface Tension on the Adhesion Performance of Nanoscale Fibrillary Structures------a Theoretical Prediction},
  author = {Zhu, Xinyao and Wang, Shaowei and Qin, Rui},
