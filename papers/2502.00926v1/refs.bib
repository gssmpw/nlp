  keywords = {1pass,annotated,elastomer,materialsscience,refparameters},
  file = {C:\Users\Katie Allison\OneDrive - University of Toronto\Documents\School\Research\Papers\Johnston_et_al_2014_Mechanical_characterization_of_bulk_Sylgard_184_for_microfluidics_and_microengineering.pdf}
