  doi = {10.1016/B978-0-32-390445-2.00017-9},
  urldate = {2023-06-23},
  abstract = {In-hand manipulation has been a long-standing challenge due to the complexity of the dexterous hand's dynamic model and the difficulty in leveraging different kinds of manipulation motions. To address these challenges, researchers have been using either dynamic modeling methods or deep reinforcement learning methods, which are limited in different ways. In this chapter, we introduce a hierarchically structured method that combines a dynamic controller for conducting primitive motions and a deep reinforcement learning network to generate the action sequence of the primitive motions. The primitive motions for the in-hand manipulation are defined heuristically, based on whether the motion aims at moving the object or changing the contact configuration of the hand. We test our method with a three-finger simulated hand, and the results show that the hand can move a pole-shaped object freely among all the possible poses in the workspace. Experiments also show that our method is robust to environmental errors, while stably grasping the object during the entire manipulation sequence. We also show that the method could generalize to objects of other shapes.},
  isbn = {978-0-323-90445-2},
  langid = {english},
  keywords = {control,hierarchical,inhandmanipulation,manipulation,toread},
  file = {C\:\\Users\\Katie Allison\\OneDrive - University of Toronto\\Documents\\School\\Research\\Papers\\Li_2022_Chapter 9 - Learning hierarchical control for robust in-hand manipulation.pdf;C\:\\Users\\Katie Allison\\Zotero\\storage\\Z7M5ENGM\\B9780323904452000179.html}
}

@article{XiaoEtAlModelingAnalysisSoft2022,
  title = {Modeling and Analysis of Soft Robotic Surfaces Actuated by Pneumatic Network Bending Actuators},
  author = {Xiao, Wei and Hu, Dean and Yang, Gang and Jiang, Chao},
  year = {2022},
  month = mar,
  journal = {Smart Materials and Structures},
  volume = {31},
  number = {5},
  pages = {055001},
  publisher = {IOP Publishing},
  issn = {0964-1726},
  doi = {10.1088/1361-665X/ac5b1d},
  urldate = {2023-06-23},
  abstract = {Soft robots are a nascent field that aims to provide a safe interaction with humans and better adaptability to unstructured environments. Many tentacle-like one-dimensional soft robots that can mimic the basic motion in nature are developed owing to ease of design and fabrication. To expand the spectrum of soft robots, this paper gives a detailed introduction of a new type of sheet-like two-dimensional soft robot. This soft robot is called soft robotic surface (SRS), which is actuated by pneumatic network bending actuators. An analytical model of the SRS is constructed based on the minimum potential energy method, which considers both its geometry complexity and material nonlinearity. The comparisons among the analytical, experimental, and numerical results demonstrate that the analytical model can accurately predict the SRS deformation. The maximum root mean squared error for the surface morphing is 3.429 mm, which is less than 5\% of the maximum displacement for the free end. The effects of the actuating pressure and structural parameter on the SRS deformation are also investigated. The results reveal that the deformation shape of the SRS can be reconfigured by controlling the applied pressure. And the bending angle of the two actuators both decreases with the increase of the width and thickness of the soft surface. The SRS extends the research on soft robots and the developed analytical model also solves the fundamental problem of how to programme the surface morphing of soft robot surfaces. Finally, we fabricate a soft gripper that can grasp object objects with different sizes, shapes, and stiffness, which demonstrates the application of the SRS.},
  langid = {english},
  keywords = {1pass,annotated,control,elastomer,grippers,inflation,pneumatic,pressure,softgripper,softrobotics},
  file = {C:\Users\Katie Allison\OneDrive - University of Toronto\Documents\School\Research\Papers\Xiao_et_al_2022_Modeling_and_analysis_of_soft_robotic_surfaces_actuated_by_pneumatic_network_bending_actuators.pdf}
}

@article{MizushimaEtAlDeformableFingertipFriction2019,
