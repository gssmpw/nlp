}

@article{JiEtAlBioinspiredSmartSurface2022,
  title = {Bio-Inspired Smart Surface to Achieve Controllable Locomotion through Adjustable Anisotropic Friction},
  author = {Ji, Zhongying and Qin, Shiyu and Ma, Shuanhong and Jia, Xin and Wang, Xiaolong and Zhou, Feng},
