@inproceedings{YamileSandoval-CastroEtAlKirigamiSoftElastomeric2023,
  title = {A {{Kirigami}}--like {{Soft Elastomeric Skin}}: {{Design}} and~{{Influence Evaluation}} in~the~{{Mobility}} of~a~{{Bio}}--Inspired {{Snake}}--{{Arm Robot}}},
