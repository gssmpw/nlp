  issn = {2223-7690, 2223-7704},
  doi = {10.1007/s40544-021-0520-6},
  urldate = {2024-08-04},
  abstract = {Anisotropic friction generated by microstructured surfaces is crucial for performing functions such as directional locomotion and adhesion in biological systems. Hence, an epoxy-based shape memory polymer (SMP) incorporating Fe3O4 nanoparticles is used in this study to create a smart surface with oriented structures to mimic anisotropic friction and exploit human-developed controllable locomotion systems. Applying the specific properties of the epoxy-based SMP, fast switching friction can be achieved by adjusting the topography and stiffness of the microstructures on the surface. In addition, the photothermogenesis effect of Fe3O4 nanoparticles induces changes in the asymmetric topography and stiffness on the SMP surface under the irradiation of near-infrared (NIR) light, thereby inducing a rapid switching of the friction force. Furthermore, a microbot is created to demonstrate remotely controlled locomotion, such as unidirectional and round-trip movements, and braking by switching the friction force under NIR light. These results are promising for the design of new intelligent surfaces and interfaces; additionally, they may facilitate the investigation of biological structures and processes.},
  langid = {english},
  keywords = {1pass,activesurface,adaptivefriction,adaptivesurface,bioinspired,friction,locomotion,robotics,smartmaterials,spine},
  file = {C:\Users\Katie Allison\OneDrive - University of Toronto\Documents\School\Research\Papers\Ji_et_al_2022_Bio-inspired_smart_surface_to_achieve_controllable_locomotion_through_adjustable_anisotropic.pdf}
}

@article{WangEtAlBioinspiredFootedSoft2020,
  title = {Bioinspired Footed Soft Robot with Unidirectional All-Terrain Mobility},
  author = {Wang, Xin and Yang, Baisong and Tan, Di and Li, Qian and Song, Bin and Wu, Zhong-Shuai and {del Campo}, Ar{\'a}nzazu and Kappl, Michael and Wang, Zuankai and Gorb, Stanislav N. and Liu, Sheng and Xue, Longjian},
  year = {2020},
  month = may,
  journal = {Materials Today},
  volume = {35},
  pages = {42--49},
  issn = {1369-7021},
  doi = {10.1016/j.mattod.2019.12.028},
  urldate = {2024-08-04},
  abstract = {Soft robots consisting of stimuli-responsive soft materials are expected to achieve tasks otherwise impossible by robots with conventional rigid counterparts. In spite of good progress made over the past several years, robot mobility on various surfaces remains challenging. Here, we report a footed soft robot with good terrain adaptability and large load carrying capability by mimicking the anisotropic friction of gecko setae and the gait of caterpillars. The robot, termed as Geca-Robot, is composed of gecko-inspired triangular micropillars as the feet and alternating cuboids of polydimethylsiloxane (PDMS) and graphene--PDMS as the muscle. Geca-Robot is remotely powered by light with wavelengths ranging from ultraviolet to infrared, and moves with a caterpillar-like gait. The gecko-inspired feet allows Geca-Robot to unidirectionally travel on terrains of varying roughness, slope, and dryness with a wide working temperature range, and to carry loads weighing approximately 50 times its own mass. Geca-Robot will inspire the creation of further soft robot designs for various natural terrains.},
  keywords = {!important,adaptiveadhesion,locomotion,microtopography,robotics,softrobotics,toread},
  file = {C\:\\Users\\Katie Allison\\OneDrive - University of Toronto\\Documents\\School\\Research\\Papers\\Wang_et_al_2020_Bioinspired_footed_soft_robot_with_unidirectional_all-terrain_mobility.pdf;C\:\\Users\\Katie Allison\\Zotero\\storage\\6YJ32LHL\\S1369702120300018.html}
}

