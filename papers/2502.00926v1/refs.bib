@phdthesis{LaquaDynamicMicrotopographyArrays2019,
  type = {Thesis},
  title = {Dynamic {{Microtopography Arrays}} for {{Hybrid}} and {{Adaptive Switchable Adhesion}}},
  author = {Laqua, Kurtis},
