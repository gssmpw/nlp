}

@article{ZhangEtAlMultibioinspiredSoftGrasping2022,
  title = {Multibioinspired {{Soft Grasping Actuators}} with {{Laser-Induced Multiscale Microstructures}}},
  author = {Zhang, Chenchu and Zhao, Linhan and Chen, Renfei and Cao, Rui and Zhao, Qiangqiang and Wang, Chaowei and Hu, Ying and Wu, Dong},
