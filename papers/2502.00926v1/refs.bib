}

@article{GaudeniEtAlGraspingSoftPadSoft2020,
  title = {Grasping {{With}} the {{SoftPad}}, a {{Soft Sensorized Surface}} for {{Exploiting Environmental Constraints With Rigid Grippers}}},
  author = {Gaudeni, Chiara and Pozzi, Maria and Iqbal, Zubair and Malvezzi, Monica and Prattichizzo, Domenico},
