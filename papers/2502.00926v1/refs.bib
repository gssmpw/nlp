@inproceedings{TaEtAlDesignFrictional2DAnisotropy2018,
  title = {Design of {{Frictional 2D-Anisotropy Surface}} for {{Wriggle Locomotion}} of {{Printable Soft-Bodied Robots}}},
  booktitle = {2018 {{IEEE International Conference}} on {{Robotics}} and {{Automation}} ({{ICRA}})},
  author = {Ta, Tung D. and Umedachi, Takuya and Kawahara, Yoshihiro},
