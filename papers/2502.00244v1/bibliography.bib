@article{capel_longitudinal_1991,
	title = {A {LONGITUDINAL} {STUDY} {OF} {BURNOUT} {IN} {TEACHERS}},
	volume = {61},
	copyright = {http://onlinelibrary.wiley.com/termsAndConditions\#vor},
	issn = {0007-0998, 2044-8279},
	url = {https://bpspsychub.onlinelibrary.wiley.com/doi/10.1111/j.2044-8279.1991.tb00959.x},
	doi = {10.1111/j.2044-8279.1991.tb00959.x},
	abstract = {The purpose of this study was to look at burnout over one school year. Six hundred and forty teachersin 8Omiddle. upper, high schoolsand sixthformcollegein oneLocal Education Authority were sent a questionnaire in September. 1987, February, 1988 and June, 1988. Each questionnaire contained the Maslach Bumout Inventory to assess burnout at three stages of the year, as well as other scales unique to that questionnaire. Results showed no significant correlationsnor differencesbetweenburnout at the threetimesof year, althoughfor most teachers highest burnout was found in February. Profile analysis showed more teachers increased than decreased burnout over the year. A large percentage stayed at the same level. Some teachers showed a different pattern over the year. The direction of change (if any) was not necessarily the same between the three administrationsof the questionnaire. Thus, burnout was not always a process of “wearing down” over time; some teachers rejuvenated over the year. Because there was no consistency in the development of burnout over time, steps cannot be taken to reduce bumout based on an establishedpattern.Individual waysof reducing or eliminatingburnout have to be found.},
	language = {en},
	number = {1},
	urldate = {2024-08-02},
	journal = {British Journal of Educational Psychology},
	author = {Capel, Susan A.},
	month = feb,
