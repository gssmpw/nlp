	author = {Dang, Brandon and Riedl, Martin J. and Lease, Matthew},
	month = jan,
	year = {2020},
	note = {arXiv:1804.10999 [cs]},
	keywords = {Computer Science - Human-Computer Interaction},
	annote = {Comment: To be presented at the 6th AAAI Conference on Human Computation and Crowdsourcing (HCOMP 2018) and the 6th ACM Collective Intelligence Conference (CI 2018)},
	file = {arXiv Fulltext PDF:/Users/nathan/Zotero/storage/NX9URDIH/Dang et al. - 2020 - But Who Protects the Moderators The Case of Crowd.pdf:application/pdf;arXiv.org Snapshot:/Users/nathan/Zotero/storage/C4SG484A/1804.html:text/html},
