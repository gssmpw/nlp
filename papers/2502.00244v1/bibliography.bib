	pages = {1--19},
	file = {Available Version (via Google Scholar):/Users/nathan/Zotero/storage/43RCZWBA/Cook et al. - 2022 - Awe Versus Aww The Effectiveness of Two Kinds of .pdf:application/pdf},
}

