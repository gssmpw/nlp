	file = {Capel - 1991 - A LONGITUDINAL STUDY OF BURNOUT IN TEACHERS.pdf:C\:\\Users\\alext\\Zotero\\storage\\YC5YHIYN\\Capel - 1991 - A LONGITUDINAL STUDY OF BURNOUT IN TEACHERS.pdf:application/pdf},
