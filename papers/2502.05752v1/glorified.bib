@article{wu2007arxiv,
  author   = {S.G. Wu and F.S. Bao and E.Y. Xu and Y. Wang and Y. Chang and Q. Xiang},
  title    = {{A Leaf Recognition Algorithm for Plant Classification Using Probabilistic Neural Network}},
