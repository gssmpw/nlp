  abstract = {The rapid growth of Internet-based applications, such as social media platforms and blogs, has led to an abundance of comments and reviews about various topics. Sentiment analysis is the process of gathering and analyzing opinions, thoughts, and impressions regarding products, services, and issues. This technique helps corporations, governments, and individuals collect information for decision-making. However, sentiment analysis faces numerous challenges, including accurately interpreting sentiments and determining polarity. The article provides a comprehensive overview of sentiment analysis methods, applications, and challenges, offering a comparison of existing approaches, their advantages and disadvantages, and discussing the challenges and future directions for sentiment analysis.},
  issn = {1573-7462},
  url = {https://doi.org/10.1007/s10462-022-10144-1},
  doi = {10.1007/s10462-022-10144-1}
}

