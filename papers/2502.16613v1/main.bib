@article{Clancey_1986, title={From Guidon to Neomycin and Heracles in Twenty Short Lessons}, volume={7}, url={https://ojs.aaai.org/aimagazine/index.php/aimagazine/article/view/548}, DOI={10.1609/aimag.v7i3.548}, abstractNote={I review the research leading from the GUIDON rule-based tutoring system, including the reconfiguration of MYCIN into NEOMYCIN and NEOMYCIN’s generalization in the heuristic classification shell, HERACLES. The presentation is organized chronologically around pictures and dialogues that represent conceptual turning points and crystallize the basic ideas. My purpose is to collect the important results in one place, so they can be easily grasped. In the conclusion, I make some observations about our research methodology.}, number={3}, journal={AI Magazine}, author={Clancey, William J.}, year={1986}, month={Jul.}, pages={40} }

