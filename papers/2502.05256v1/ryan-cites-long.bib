@article{ms_scope,
	series = {{VLDB} '08},
	title = {{SCOPE}: easy and efficient parallel processing of massive data sets},
	volume = {1},
	issn = {2150-8097},
	shorttitle = {{SCOPE}},
	url = {https://dl.acm.org/doi/10.14778/1454159.1454166},
