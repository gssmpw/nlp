	pages = {191--202},
	file = {Full Text PDF:/home/ryan/Zotero/storage/VNK988K8/Manegold et al. - 2002 - Generic database cost models for hierarchical memo.pdf:application/pdf},
}

@article{logic_bugs_qo,
