	month = nov,
	year = {2016},
	pages = {689--703},
	file = {Arnautov et al. - 2016 - SCONE secure Linux containers with Intel SGX.pdf:/home/ryan/Zotero/storage/GX53E5PH/Arnautov et al. - 2016 - SCONE secure Linux containers with Intel SGX.pdf:application/pdf},
