@techreport{giakoumakisTestingSQLServer2008,
	title = {Testing {SQL} {Server}'s {Query} {Optimizer}: {Challenges}, {Techniques} and {Experiences}},
	shorttitle = {Testing {SQL} {Server}'s {Query} {Optimizer}},
