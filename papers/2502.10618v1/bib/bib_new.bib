@inproceedings{LojoFox_TestingPlan_ITiCSE-2022,
	address = {New York, NY, USA},
	series = {{ITiCSE} '22},
	title = {Teaching {Test}-{Writing} {As} a {Variably}-{Scaffolded} {Programming} {Pattern}},
	isbn = {978-1-4503-9201-3},
