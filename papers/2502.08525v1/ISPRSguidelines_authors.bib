@article{jansen_decreasing_2019,
	title = {Decreasing the {Uncertainty} of the {Target} {Center} {Estimation} at {Terrestrial} {Laser} {Scanning} by {Choosing} the {Best} {Algorithm} and by {Improving} the {Target} {Design}},
	volume = {11},
	copyright = {http://creativecommons.org/licenses/by/3.0/},
	issn = {2072-4292},
	doi = {https://www.mdpi.com/2072-4292/11/7/845},
	url = {doi.org/10.3390/rs11070845},
	abstract = {During the registration and georeferencing of terrestrial laser scans, it is common to use targets to mark discrete points. To improve the accuracy of the registration, the uncertainties of the target center estimation (TCE) have to be minimized. The present study examines different factors influencing the precision of the TCE. Here, the focus is on the algorithm and the target design. It is determined that, in general, the uncertainties of the TCE are much smaller than those indicated by the manufacturers. By comparing different algorithms for the first time, it was possible to clearly determine that an algorithm using image correlations yields the smallest standard deviations for the TCE. A comparison of different target designs could not identify an ideal commercially available target. For this reason, a new target, the BOTA8 (BOnn TArget with 8-fold pattern) was developed, which leads to smaller standard deviations than the previous targets. By choosing the best algorithm and improving the target design, standard deviations of     0.5     mm in distance direction and     1.2     arcsec in angular direction for a scan distance up to 100 m were achieved with the laser scanner Leica ScanStation P20. The uncertainties could be reduced by several millimetres and angular seconds compared to the manufacturer’s targets and software.},
	language = {en},
	number = {7},
	urldate = {2024-10-11},
	journal = {Remote Sensing},
	author = {Janßen, Jannik and Medic, Tomislav and Kuhlmann, Heiner and Holst, Christoph},
	month = jan,
	year = {2019},
	pages = {845},
	file = {Full Text PDF:C\:\\Users\\ucesjo0\\Zotero\\storage\\T4EW7SH8\\Janßen et al. - 2019 - Decreasing the Uncertainty of the Target Center Es.pdf:application/pdf},
}


