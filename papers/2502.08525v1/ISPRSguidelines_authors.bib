	year = {2013},
	note = {Conference Name: WG V/3, I/2, I/3, III/2, V/2, VII/7, ICWG I/Va {\textless}/br{\textgreater} ISPRS Workshop Laser Scanning 2013 (Volume II-5/W2) - 11\&ndash;13 November 2013, Antalya, Turkey
Publisher: Copernicus GmbH},
	keywords = {calibration, planar target, point cloud, registration, target detection, Terrestrial laser scanner},
	pages = {139--144},
	file = {Full Text PDF:C\:\\Users\\ucesjo0\\Zotero\\storage\\PN83D3TS\\Kregar et al. - 2013 - High precision target center determination from a .pdf:application/pdf},
