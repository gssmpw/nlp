	file = {Jiang et al_2018_Deep ROI-Based Modeling for Urban Human Mobility Prediction.pdf:files/6026/Jiang et al_2018_Deep ROI-Based Modeling for Urban Human Mobility Prediction.pdf:application/pdf;Jiang et al_2018_Deep ROI-Based Modeling for Urban Human Mobility Prediction.pdf:files/6041/Jiang et al_2018_Deep ROI-Based Modeling for Urban Human Mobility Prediction.pdf:application/pdf},
