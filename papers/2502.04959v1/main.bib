	note = {Conference Name: IEEE Journal of Selected Topics in Applied Earth Observations and Remote Sensing},
	keywords = {Benchmark testing, Dataset, deep convolutional neural network, deep learning, Earth, earth observation, Feature extraction, land cover classification, land use classification, machine learning, Machine learning, remote sensing, Remote sensing, satellite image classification, satellite images, Satellites, Spatial resolution},
	pages = {2217--2226},
	file = {IEEE Xplore Abstract Record:files/1154/8736785.html:text/html;IEEE Xplore Full Text PDF:files/1150/Helber et al. - 2019 - EuroSAT A Novel Dataset and Deep Learning Benchma.pdf:application/pdf},
