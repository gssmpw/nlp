@inproceedings{krause_3d_2013,
	address = {Sydney, Australia},
	title = {{3D} {Object} {Representations} for {Fine}-{Grained} {Categorization}},
	isbn = {978-1-4799-3022-7},
	url = {http://ieeexplore.ieee.org/document/6755945/},
	doi = {10.1109/ICCVW.2013.77},
	abstract = {While 3D object representations are being revived in the context of multi-view object class detection and scene understanding, they have not yet attained wide-spread use in ﬁne-grained categorization. State-of-the-art approaches achieve remarkable performance when training data is plentiful, but they are typically tied to ﬂat, 2D representations that model objects as a collection of unconnected views, limiting their ability to generalize across viewpoints. In this paper, we therefore lift two state-of-the-art 2D object representations to 3D, on the level of both local feature appearance and location. In extensive experiments on existing and newly proposed datasets, we show our 3D object representations outperform their state-of-the-art 2D counterparts for ﬁne-grained categorization and demonstrate their efﬁcacy for estimating 3D geometry from images via ultrawide baseline matching and 3D reconstruction.},
	language = {en},
	urldate = {2024-09-02},
	booktitle = {2013 {IEEE} {International} {Conference} on {Computer} {Vision} {Workshops}},
	publisher = {IEEE},
	author = {Krause, Jonathan and Stark, Michael and Deng, Jia and Fei-Fei, Li},
	month = dec,
	year = {2013},
	pages = {554--561},
	file = {Krause et al. - 2013 - 3D Object Representations for Fine-Grained Categor.pdf:files/1147/Krause et al. - 2013 - 3D Object Representations for Fine-Grained Categor.pdf:application/pdf},
}

