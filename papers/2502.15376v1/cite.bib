@article{Jin2023devices,
title = {Topological quantum devices: a review},
author = {Jin, Kyung-Hwan and Jiang, Wei and Sethi, Gurjyot and Liu, Feng},
abstractNote = {The introduction of the concept of topology into condensed matter physics has greatly deepened our fundamental understanding of transport properties of electrons as well as all other forms of quasi particles in solid materials. It has also fostered a paradigm shift from conventional electronic/optoelectronic devices to novel quantum devices based on topology-enabled quantum device functionalities that transfer energy and information with unprecedented precision, robustness, and efficiency. In this article, the recent research progress in topological quantum devices is reviewed. We first outline the topological spintronic devices underlined by the spin-momentum locking property of topology. We then highlight the topological electronic devices based on quantized electron and dissipationless spin conductivity protected by topology. Finally, we discuss quantum optoelectronic devices with topology-redefined photoexcitation and emission. The field of topological quantum devices is only in its infancy, we envision many significant advances in the near future.},
doi = {10.1039/d3nr01288c},
journal = {Nanoscale},
number = 31,
volume = 15,
place = {United States},
year = {2023},
month = {7}
}


