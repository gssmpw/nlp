@Article{D2CP05893F,
author ="Olejniczak, Małgorzata and Tierny, Julien",
title  ="Topological data analysis of vortices in the magnetically-induced current density in LiH molecule",
journal  ="Phys. Chem. Chem. Phys.",
year  ="2023",
volume  ="25",
issue  ="8",
pages  ="5942-5947",
publisher  ="The Royal Society of Chemistry",
doi  ="10.1039/D2CP05893F",
@url  ="http://dx.doi.org/10.1039/D2CP05893F",
abstract  ="A novel strategy for extracting axial (AV) and toroidal (TV) vortices in the magnetically-induced current density (MICD) in molecular systems is introduced{,} and its pilot application to LiH molecule is demonstrated. It exploits differences in the topologies of AV and TV cores and involves two key steps: selecting a scalar function that can describe vortex cores in MICD and its subsequent topological analysis. The scalar function of choice is ΩBα based on the velocity-gradient Ω method known in research on classical flows. The Topological Data Analysis (TDA) is then used to analyze the ΩBα scalar field. In particular{,} TDA robustly assigns distinct topological features of this field to different vortex types in LiH: AV to saddle-maximum separatrices which connect maxima to 2-saddles located on the domain{'}s boundary{,} TV to a 1-cycle of the super-level sets of the input data. Both are extracted as the most persistent features of the topologically-simplified ΩBα scalar field."}

