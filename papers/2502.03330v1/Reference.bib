@inproceedings{ledo_evaluation_2018,
	address = {Montreal QC Canada},
	title = {Evaluation {Strategies} for {HCI} {Toolkit} {Research}},
	isbn = {978-1-4503-5620-6},
	url = {https://dl.acm.org/doi/10.1145/3173574.3173610},
