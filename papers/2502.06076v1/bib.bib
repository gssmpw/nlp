@article{BrownLoZh97,
author = {Lawrence D. Brown and Mark G. Low and Linda H. Zhao},
title = {Superefficiency in nonparametric
function estimation},
year = 1997,
journal = aos,
volume = 25,
number = 6,
pages = {2607--2625},
comment = {Shows that pointwise notions of risk are problematic for
  nonparametric function estimators. In particular, consider the Sobolev
  elliptical ball $\{\theta \mid \sum_{j = 1}^\infty j^{2r} \theta_j^2
  \le B^2 \}$, which has minimax rates $n^{-2r / (2r + 1)}$ in mean squared
  error. (With known constants depending on $B$.) Shows that there
  exist linear estimators that, for any given $\theta$ with
  Sobolev norm $\norm{\theta}$ and $\epsilon > 0$, there is a linear esitmator
  with pointwise risk, relative to the minimax rate,
  at most $\epsilon$. Also exhibits a Stein-type estimator
  that achieves asymptotic relative risk $0$.
  }
}

