@article{ToulisHoAi15,
title={Stable {R}obbins-{M}onro approximations
  through stochastic proximal updates},
author={Toulis, Panos and Horel, Thibaut and Airoldi, Edoardo M},
year = 2015,
journal = {arXiv:1510.00967 [math.ST]},
comment = {Investigates iterative procedures that apply
  $\theta_n = \theta_{n-1} - \gamma_n h(\theta_n)$, where $h$ is the
  stochastic gradient. Make the ``weakened'' assumption (not really weaker)
  that $\theta_n = \theta_{n-1} - \gamma_n W(\theta_n^+)$, where $W$
  is the random gradient and $\E[\theta_n \mid \mc{F}_{n-1}] = \theta_n^+$
  so that it's some kind of stochastically unbiased proximal update.
  I cannot see situations where that would happen.
  Additionally, makes the assumption (Assumption 5) that the noise
  has bounded L2 norm everywhere, which of course is unrealistic unless
  one can prove such a thing. (For example, linear regression
  fails this.) Proves convergence guarantees for final iterates (and
  rates) based on stepsize powers. Also shows asymptotic normality
  of final iterate under some strong assumptions.}
}

@inproceedings{ToulisTrAi16,
author = {Panos Toulis and Dustin Tran and Edoardo Airoldi},
title = {Towards stability and optimality in stochastic gradient descent},
year = 2016,
booktitle = aistat16,
comment = {Similar to the other papers of Toulis and Airoldi, and not
  really clear what the generalization is. Still assumes that the functions
  involved are globally Lipschitz and strongly convex, which more or less
  defeats the purpose of the results. Claims of ``intuition'' from
  strongly convex case also have false proofs, and assume that random
  functions are (i) all strongly convex and (ii) have the same shared
  minimizer. In this case, of course, using infinite stepsize is optimal,
  because it minimizes all the functions simultaneously.
  (See Assumptions 3a and 4 for the contradictory assuptions.)},
}

