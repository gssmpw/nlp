}

@inproceedings{liu_what_2023,
	address = {New York, NY, USA},
	series = {{CHI} '23},
	title = {“{What} {It} {Wants} {Me} {To} {Say}”: {Bridging} the {Abstraction} {Gap} {Between} {End}-{User} {Programmers} and {Code}-{Generating} {Large} {Language} {Models}},
