	month = apr,
	year = {2023},
	keywords = {behavior change, ccfInfo: CCF-A CHI, citationNumber: 1, conceptual framework, design guideline, hybrid workshops, sedentary behavior},
	pages = {1--7},
}

@inproceedings{wang_enabling_2023,
