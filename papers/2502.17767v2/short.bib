@article{GKS23,
  author =        {Gao, Yuan and Kyng, Rasmus and Spielman, Daniel A.},
  month =         jun,
  url = {https://arxiv.org/abs/2303.00709v2},
  journal =        {arXiv preprint arXiv:2303.00709},
  title =         {Robust and Practical Solution of {{Laplacian}}
                   Equations by Approximate Elimination},
  year =          {2023},
  abstract =      {We introduce a new algorithm and software for solving
                   linear equations in symmetric diagonally dominant
                   matrices with non-positive off-diagonal entries (SDDM
                   matrices), including Laplacian matrices. We use
                   preconditioned conjugate gradient to solve the system
                   of linear equations. Our preconditioner is a variant
                   of the Approximate Cholesky factorization of Kyng and
                   Sachdeva (FOCS 2016). Our factorization approach is
                   simple: we eliminate matrix rows/columns one at a
                   time, and update the remaining entries of the matrix
                   by sampling entries to approximate the outcome of
                   complete Cholesky factorization. Unlike earlier
                   approaches, our sampled entries always maintain a
                   connected graph on the neighbors of the eliminated
                   variable. Our algorithm comes with a tuning parameter
                   that upper bounds the number of samples made per
                   original entry.},
}

