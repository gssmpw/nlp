  journal =       {SIAM Journal on Matrix Analysis and Applications},
  month =         jan,
  number =        {2},
  pages =         {524--550},
  publisher =     {{Society for Industrial and Applied Mathematics}},
  title =         {{{MIQR}}: {A} Multilevel Incomplete {QR}
                   Preconditioner for Large Sparse Least-Squares
                   Problems},
  volume =        {28},
  year =          {2006},
  abstract =      {This paper describes a technique for constructing
                   robust preconditioners for the CGLS method applied to
                   the solution of large and sparse least squares
                   problems. The algorithm computes an incomplete LDLT
                   factorization of the normal equations matrix without
                   the need to form the normal matrix itself. The
                   preconditioner is reliable (pivot breakdowns cannot
                   occur) and has low intermediate storage requirements.
                   Numerical experiments illustrating the performance of
                   the preconditioner are presented. A comparison with
                   incomplete QR preconditioners is also included.},
  doi =           {10.1137/050633032},
  issn =          {0895-4798},
}

