  journal =       {Acta Numerica},
  month =         may,
  pages =         {403--572},
  title =         {Randomized Numerical Linear Algebra: {{Foundations}}
                   and Algorithms},
  volume =        {29},
  year =          {2020},
  abstract =      {This survey describes probabilistic algorithms for
                   linear algebraic computations, such as factorizing
                   matrices and solving linear systems. It focuses on
                   techniques that have a proven track record for
                   real-world problems. The paper treats both the
                   theoretical foundations of the subject and practical
                   computational issues.Topics include norm estimation,
                   matrix approximation by sampling, structured and
                   unstructured random embeddings, linear regression
                   problems, low-rank approximation, subspace iteration
                   and Krylov methods, error estimation and adaptivity,
                   interpolatory and CUR factorizations, Nystr{\"o}m
                   approximation of positive semidefinite matrices,
                   single-view (`streaming') algorithms, full
                   rank-revealing factorizations, solvers for linear
                   systems, and approximation of kernel matrices that
                   arise in machine learning and in scientific
                   computing.},
  doi =           {10.1017/S0962492920000021},
  issn =          {0962-4929, 1474-0508},
}

