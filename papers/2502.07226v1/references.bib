  shorttitle = {{{InfoGAN}}},
  booktitle = {Advances in {{Neural Information Processing Systems}}},
  author = {Chen, Xi and Duan, Yan and Houthooft, Rein and Schulman, John and Sutskever, Ilya and Abbeel, Pieter},
