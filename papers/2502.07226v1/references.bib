@article{zhouAggregatedFeasibleActive2024,
  title = {Aggregated {{Feasible Active Power Region}} for {{Distributed Energy Resources}} with a {{Distributionally Robust Joint Probabilistic Guarantee}}},
  author = {Zhou, Yihong and Essayeh, Chaimaa and Morstyn, Thomas},
  year = {2024},
