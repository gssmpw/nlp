@phdthesis{wangFlexibilityAggregationCoordination2024,
  title = {{Flexibility Aggregation and Coordination of Diverse Energy Resources}},
  author = {Wang, Siyuan},
  year = {2024},
  month = may,
  address = {Tsinghua University},
  abstract = {As the proportion of renewable energy generation, characterized by high randomness and volatility, gradually increases, the power system requires more flexible resources to maintain supply-demand balance, and ensure secure and stable operation. The flexibility of the power system comes from three main aspects: distributed energy resources in the distribution network, multi-energy flexibility of gas and heat network, and support from neighboring regional power grids. Diverse energy systems comprise numerous controllable objects, leading to the curse of dimensionality in optimal dispatch and control. This thesis proposes flexible aggregation reference models and calculation methods for diverse energy systems, constructing equivalent models for flexibility aggregation in distributed generation and flexible loads, gas networks, heat networks, and regional power grid systems. Additionally, this thesis introduces an algorithm to calculate the parameters of these equivalent models, enabling coordinated system dispatch. This thesis starts by explaining the aggregation of energy system flexibility from both physical and mathematical perspectives, presenting an efficient solution for the flexibility aggregation reference model: the high-dimensional polyhedron projection inner-approximation method. This method integrates the physical significance of flexibility resource aggregation problems and employs three inner-shape template inner-approximation strategies, depending on the presence of explicit physical expressions. This results in more accurate models of energy system flexibility aggregation and effectively guarantees the feasibility of disaggregated dispatch commands. To solve the curse of dimensionality in coordinating and dispatching numerous distributed energy resources within the distribution network, this thesis proposes a method for aggregating their flexibility. Considering operational constraints, random factors, and network constraints, the thesis classifies and aggregates distributed energy resources into a flexibility aggregation model consisting of an equivalent generator and an equivalent energy storage device. This aggregated model can be applied in calculating steady-state flexibility quantification metrics for the entire distribution system and asynchronous coordination between transmission and distribution networks. To address the joint dispatch problem arising from the differing physical equations of integrated energy systems, this thesis proposes an equivalent energy storage flexibility evaluation method for gas and heat networks. Network parameters are identified from operational data, and the gas and heat networks are modeled as multi-port energy storage devices, considering dynamic processes of gas and heat flow described by partial differential equations and network constraints. The equivalent storage parameters are calculated using the high-dimensional polyhedron projection method, enabling quantitative assessment of the equivalent energy storage capacity. This approach decouples computational complexity from the size of the gas and heat networks, simplifying the multi-energy system coordination problem and enabling more efficient optimization. To address the inefficiency caused by the division of dispatch authority in multi-regional interconnected power grids, a flexible aggregation equivalent model of regional power grids composed of several equivalent generators is constructed. A method combining projection space dimension decomposition, inscribed projection approximation, and reorganization is proposed to calculate the parameters of the regional power grid flexibility aggregation model. With the equivalent aggregation model of regional power grids, efficient resolution of tie-line dispatch optimization problems among multi-area interconnected power grids is enabled, facilitating their coordination.},
  copyright = {All rights reserved},
  langid = {chinese},
  school = {Tsinghua University}
}

