@article{shiOptimalResidentialDemand2014,
  title = {Optimal {{Residential Demand Response}} in {{Distribution Networks}}},
  author = {Shi, Wenbo and Li, Na and Xie, Xiaorong and Chu, Chi-Cheng and Gadh, Rajit},
  year = {2014},
