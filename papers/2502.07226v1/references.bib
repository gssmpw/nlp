  author = {Lothon, Marie and Barn{\'e}oud, Paul and Gabella, Omar and Lohou, Fabienne and Derrien, Sol{\`e}ne and Rondi, Sylvain and Chiriaco, Marjolaine and Bastin, Sophie and Dupont, Jean-Charles and Haeffelin, Martial and Badosa, Jordi and Pascal, Nicolas and Montoux, Nad{\`e}ge},
  year = {2019},
