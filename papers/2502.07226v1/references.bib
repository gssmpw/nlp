@article{heEconomicDispatchMultiarea2021,
  title = {Economic Dispatch of Multi-Area Integrated Electricity and Natural Gas Systems Considering Emission and Hourly Spinning Reserve Constraints},
  author = {He, Liangce and Lu, Zhigang and Zhang, Jiangfeng and Geng, Lijun and Cai, Yao and Li, Xueping},
  year = {2021},
