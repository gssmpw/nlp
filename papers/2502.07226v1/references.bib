  shorttitle = {Efficient {{Acceleration}} of {{Deep Learning Inference}} on {{Resource-Constrained Edge Devices}}},
  author = {Shuvo, Md. Maruf Hossain and Islam, Syed Kamrul and Cheng, Jianlin and Morshed, Bashir I.},
  year = {2023},
