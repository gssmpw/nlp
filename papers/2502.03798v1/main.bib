@Article{transfer_flow_2,
title = {Transfer learning for robust urban network-wide traffic volume estimation with uncertain detector deployment scheme},
journal = {Electronic Research Archive},
volume = {31},
number = {1},
pages = {207-228},
year = {2023},
issn = {2688-1594},
doi = {10.3934/era.2023011},
url = {https://www.aimspress.com/article/doi/10.3934/era.2023011},
author = {Jiping Xing and Yunchi Wu and Di Huang and Xin Liu},
keywords = {network-wide volume estimation, transfer component analysis, multi-source data fusion, taxi GPS data, cellular signaling data},
abstract = {Real-time and accurate network-wide traffic volume estimation/detection is an essential part of urban transport system planning and management. As it is impractical to install detectors on every road segment of the city network, methods on the network-wide flow estimation based on limited detector data are of considerable significance. However, when the plan of detector deployment is uncertain, existing methods are unsuitable to be directly used. In this study, a transfer component analysis (TCA)-based network-wide volume estimation model, considering the different traffic volume distributions of road segments and transforming traffic features into common data space, is proposed. Moreover, this study applied taxi GPS (global positioning system) data and cellular signaling data with the same spatio-temporal coverage to improve feature extraction. In numerical experiments, the robustness and stability of the proposed network-wide estimation method outperformed other baselines in the two subnetworks selected from the urban centers and suburbs.}
}

