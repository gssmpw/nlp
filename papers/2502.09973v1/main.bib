pages = {532–538},
numpages = {7},
keywords = {Reminiscing, Tangible Interaction, Interactive Jewellery, Interaction Design, Memento, Sonic Memory Cue},
location = {Eindhoven, Netherlands},
series = {TEI '16}
}

