pages = {1555–1564},
numpages = {10},
keywords = {Shape change, designer-researcher approach, materiality, research through design, temporality},
location = {<conf-loc>, <city>Virtual Event</city>, <country>Australia</country>, </conf-loc>},
series = {DIS '22}
}

