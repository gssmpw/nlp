@inproceedings{jun19bilinear,
abstract = {We introduce the bilinear bandit problem with low-rank structure in which an action takes the form of a pair of arms from two different entity types, and the reward is a bilinear function of the known feature vectors of the arms. The unknown in the problem is a {\$}d{\_}1{\$} by {\$}d{\_}2{\$} matrix {\$}\backslashmathbf{\{}\backslashTheta{\}}{\^{}}*{\$} that defines the reward, and has low rank {\$}r \backslashll \backslashmin{\{}d{\_}1,d{\_}2{\}}{\$}. Determination of {\$}\backslashmathbf{\{}\backslashTheta{\}}{\^{}}*{\$} with this low-rank structure poses a significant challenge in finding the right exploration-exploitation tradeoff. In this work, we propose a new two-stage algorithm called “Explore-Subspace-Then-Refine” (ESTR). The first stage is an explicit subspace exploration, while the second stage is a linear bandit algorithm called “almost-low-dimensional OFUL” (LowOFUL) that exploits and further refines the estimated subspace via a regularization technique. We show that the regret of ESTR is {\$}\backslashwidetilde{\{}\backslashmathcal{\{}O{\}}{\}}((d{\_}1+d{\_}2){\^{}}{\{}3/2{\}} \backslashsqrt{\{}r T{\}}){\$} where {\$}\backslashwidetilde{\{}\backslashmathcal{\{}O{\}}{\}}{\$} hides logarithmic factors and {\$}T{\$} is the time horizon, which improves upon the regret of {\$}\backslashwidetilde{\{}\backslashmathcal{\{}O{\}}{\}}(d{\_}1d{\_}2\backslashsqrt{\{}T{\}}){\$} attained for a na{\"{i}}ve linear bandit reduction. We conjecture that the regret bound of ESTR is unimprovable up to polylogarithmic factors, and our preliminary experiment shows that ESTR outperforms a na{\"{i}}ve linear bandit reduction.},
author = {Jun, Kwang-Sung and Willett, Rebecca and Wright, Stephen and Nowak, Robert},
booktitle = {Proceedings of the International Conference on Machine Learning (ICML)},
pages = {3163--3172},
title = {{Bilinear Bandits with Low-rank Structure}},
volume = {97},
year = {2019}
}
