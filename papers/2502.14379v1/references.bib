isbn = {9781467369978},
issn = {15206149},
keywords = {Active learning,Gaussian Markov random field,Graph Signal Processing,Sampling theorem,Semi-supervised learning},
pages = {3257--3261},
title = {{A probabilistic interpretation of sampling theory of graph signals}},
