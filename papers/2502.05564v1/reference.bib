@misc{mullerTransformersCanBayesian2024,
  title = {Transformers {{Can Do Bayesian Inference}}},
  author = {M{\"u}ller, Samuel and Hollmann, Noah and Arango, Sebastian Pineda and Grabocka, Josif and Hutter, Frank},
