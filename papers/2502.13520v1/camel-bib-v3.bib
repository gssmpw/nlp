	Doi = {10.18653/v1/W19-4608},
	Month = {August},
	Pages = {68--77},
	Publisher = {Association for Computational Linguistics},
	Title = {h{ULM}on{A}: The Universal Language Model in {A}rabic},
	Url = {https://www.aclweb.org/anthology/W19-4608},
